<test xmlns="http://myfaces.apache.org/trinidad/test"
       xmlns:tr="http://myfaces.apache.org/trinidad">
  <base-component tr:type="org.apache.myfaces.trinidad.CoreChart"
                  value="#{chartModel}"/>  

  <!-- ungrouped --> 
  <attribute-test name="type" value="verticalBar" matchesBase="true"/>
  <attribute-test name="templateSource" value="/mySVGTemplate.svg"/>
  <attribute-test name="legendPosition" value="end"/>
  <attribute-test name="animationDuration" value="1500"/>
  <attribute-test name="YMajorGridLineCount" value="7"/>
  <attribute-test name="YMinorGridLineCount" value="3"/>
  <attribute-test name="XMajorGridLineCount" value="4"/>
  <attribute-test name="maxPrecision" value="1"/>

   <!-- core -->  
   <attribute-test name="id" value="myid"/>
   <attribute-test name="styleClass" value="mystyle"/>     
  <attribute-test name="inlineStyle" value="width:600px; height:400px;"/>
</test>
