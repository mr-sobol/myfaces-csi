<faces-config xmlns="http://java.sun.com/JSF/Configuration">
   <component>
      <description>The TableFormLayout .</description>
      <component-type>org.apache.myfaces.trinidad.CoreTableFormLayout</component-type>
      <component-class>org.apache.myfaces.trinidad.component.core.layout.CoreTableFormLayout</component-class><!--org.apache.myfaces.trinidad.CoreTableFormLayout facets-->
